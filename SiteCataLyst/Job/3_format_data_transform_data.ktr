        <class_source><![CDATA[import java.util.*;
import java.text.SimpleDateFormat;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r=getRow();
  if (r==null)
  {
    setOutputDone();
	return false;
  }
  if (first) {
     first=false;
  }
  Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
  int outputListFilesInfex = getInputRowMeta().size();
// start code
	// convert date
	String date = get(Fields.In, "﻿date").getString(r);

	Date formatted_date = new Date();
	String converted_date = "";

	SimpleDateFormat formatter = new SimpleDateFormat("yyyyMMdd");
	try {
    if (date.matches("\\d+")) {
    	int serialDate = Integer.parseInt(date);
    	Calendar baseDate = Calendar.getInstance();
    	baseDate.set(1899, Calendar.DECEMBER, 30);
    	baseDate.add(Calendar.DATE, serialDate);
        converted_date = formatter.format(baseDate.getTime()); 
    } else {
    String[] parts = date.split("/");
    int year = Integer.parseInt(parts[0]);
    int month = Integer.parseInt(parts[1]) - 1; 
    int dayOfYear = Integer.parseInt(parts[2]);
    Calendar resultDate = Calendar.getInstance();
    resultDate.set(year, Calendar.JANUARY, 1);
    resultDate.add(Calendar.DATE, dayOfYear - 1); 
	SimpleDateFormat sdf = new SimpleDateFormat("yyyyMMdd");
    converted_date = formatter.format(resultDate.getTime());
    }
	formatted_date = formatter.parse(converted_date);
	} catch (Exception e) {
		e.printStackTrace();
	}
	// create another field
	String Device = "Other";
	String eventName = get(Fields.In, "eventName").getString(r);
	Long counts = get(Fields.In, "counts").getInteger(r);
	Double CV01 = 0.0;
	Double CV02 = 0.0;
	Double CV04 = 0.0;
	if(!eventName.equals("event_1") && !eventName.equals("event_2") && !eventName.equals("event_3")){
		CV04 = 1.0;
	}
	else if(eventName.equals("event_1")){
		CV01 = counts.doubleValue();
	}else if(eventName.equals("event_2")){
		CV02 = counts.doubleValue();
	}
// end code
  outputRowData[outputListFilesInfex ] = formatted_date ;
  outputRowData[++outputListFilesInfex ] = Device ;
  outputRowData[++outputListFilesInfex ] = CV01 ;
  outputRowData[++outputListFilesInfex ] = CV02 ;
  outputRowData[outputListFilesInfex + 2 ] = CV04 ;
  putRow(data.outputRowMeta, outputRowData);
  return true;
}]]></class_source>
