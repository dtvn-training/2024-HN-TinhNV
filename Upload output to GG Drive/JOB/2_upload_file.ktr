        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.io.*;
import java.net.HttpURLConnection;
import java.net.URL;
import java.util.*;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r=getRow();
  if (r==null)
  {
    setOutputDone();
	return false;
  }
  if (first) {
     first=false;
  }
  Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
  int outputListFilesInfex = getInputRowMeta().size();
// start code
	String access_token = get(Fields.In, "access_token").getString(r);
	String GG_DRIVE_UPLOAD_URL = getVariable("GG_DRIVE_UPLOAD_URL","r");
	String FOLDER_ID = getVariable("FOLDER_ID","r");
	String FILE_PATH = getVariable("FILE_PATH","r");
	String fileName = "r";
	String responseMess = "";
	try {
        URL url = new URL(GG_DRIVE_UPLOAD_URL);
        HttpURLConnection connection = (HttpURLConnection) url.openConnection();
        connection.setRequestMethod("POST");
        connection.setDoOutput(true);
        connection.setRequestProperty("Authorization", "Bearer " + access_token);
        connection.setRequestProperty("Content-Type", "multipart/related; boundary=foo_bar_baz");

        String boundary = "foo_bar_baz";
        String newLine = "\r\n";
        fileName = FILE_PATH.substring(FILE_PATH.lastIndexOf('/') + 1);

        OutputStream outputStream = connection.getOutputStream();
        DataOutputStream writer = new DataOutputStream(outputStream);

        writer.writeBytes("--" + boundary + newLine);
        writer.writeBytes("Content-Type: application/json; charset=UTF-8" + newLine);
        writer.writeBytes(newLine);

		String metaData = "{" + "\"name\": \"" + fileName + "\"," + "\"parents\": [\"" + FOLDER_ID + "\"]" + "}";
        writer.writeBytes(metaData);
        writer.writeBytes(newLine);

		writer.writeBytes("--" + boundary + newLine);
        writer.writeBytes("Content-Type: application/text-plain" + newLine);
		writer.writeBytes(newLine);

        FileInputStream fileInputStream = new FileInputStream(FILE_PATH);
        byte[] buffer = new byte[4096];
        int bytesRead = -1;
        while ((bytesRead = fileInputStream.read(buffer)) != -1) {
            writer.write(buffer, 0, bytesRead);
        }
		writer.writeBytes(newLine);
        writer.writeBytes("--" + boundary + "--" + newLine);

		int responseCode = connection.getResponseCode();
		Scanner scanner = new Scanner(connection.getInputStream());
		while(scanner.hasNext()){
			responseMess += scanner.nextLine(); 
		}
		//logBasic(responseMess);
        if (responseCode == HttpURLConnection.HTTP_OK){
			logBasic("DEV: OK");
		}
		else {
			logBasic("DEV: FAILED");
		}

    } catch (IOException e) {
        e.printStackTrace();
    }
// end code
  outputRowData[outputListFilesInfex ] = GG_DRIVE_UPLOAD_URL;
  outputRowData[++outputListFilesInfex ] = FOLDER_ID;
  outputRowData[++outputListFilesInfex ] = FILE_PATH;
  outputRowData[++outputListFilesInfex ] = fileName;
  outputRowData[++outputListFilesInfex ] = responseMess;
  putRow(data.outputRowMeta, outputRowData);
  return true;
}]]></class_source>
