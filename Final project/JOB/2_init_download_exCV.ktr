        <class_source><![CDATA[import java.util.*;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r=getRow();
  if (r==null)
  {
    setOutputDone();
	return false;
  }
  if (first) {
     first=false;
  }
  Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
  int outputListFilesInfex = getInputRowMeta().size();
// start code
	String name = get(Fields.In, "name").getString(r);
	String id = get(Fields.Out, "id").getString(r);
	String NB_INPUT_FILE_NAME = getVariable("NB_INPUT_FILE_NAME", "filename");
	String QUOTE_INPUT_FILE_NAME = getVariable("QUOTE_INPUT_FILE_NAME", "filename");
	String GG_DRIVE_DOWNLOAD_FILE_URL = getVariable("GG_DRIVE_DOWNLOAD_FILE_URL","r");
	String download_URL = "";

	String[] inputNBArr = NB_INPUT_FILE_NAME.split("\\|");
	String[] inputQuoteArr = QUOTE_INPUT_FILE_NAME.split("\\|");
	for(int i = 0; i < inputNBArr.length ; i++){
		if(inputNBArr[i].equals(name) || inputQuoteArr[i].equals(name)){
			download_URL = GG_DRIVE_DOWNLOAD_FILE_URL.replace("fileId",id);
		}
	}
	
// end code

  outputRowData[outputListFilesInfex ] = download_URL;
  putRow(data.outputRowMeta, outputRowData);
  return true;
}]]></class_source>
