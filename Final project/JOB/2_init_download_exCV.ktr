        <class_source><![CDATA[import java.util.*;
import javax.xml.bind.DatatypeConverter;
import java.util.Calendar;
import java.util.Date;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r=getRow();
  if (r==null)
  {
    setOutputDone();
	return false;
  }
  if (first) {
     first=false;
  }
  Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
  int outputListFilesInfex = getInputRowMeta().size();
// start code
	String createdTime = get(Fields.In, "createdTime").getString(r);
	String modifiedTime = get(Fields.In, "modifiedTime").getString(r);
	Date updated_time = new Date();
    Calendar calendar = DatatypeConverter.parseDateTime(createdTime);
    Date created_time = calendar.getTime();
    calendar = DatatypeConverter.parseDateTime(modifiedTime);
    Date modified_time = calendar.getTime();
    if(created_time.compareTo(modified_time) > 0) {
        updated_time = created_time;
    }
    else {
        updated_time = modified_time;
    }	
	
	
// end code
  outputRowData[outputListFilesInfex ] = updated_time;
  putRow(data.outputRowMeta, outputRowData);
  return true;
}]]></class_source>
