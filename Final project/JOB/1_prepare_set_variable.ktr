        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.*;
import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;
import java.util.Calendar;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
  Object[] r=getRow();
  if (r==null)
  {
    setOutputDone();
	return false;
  }
  if (first) {
     first=false;
  }
  Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
  int outputListFilesInfex = getInputRowMeta().size();
// start code
	String date = get(Fields.In, "date").getString(r);
	Date today = new Date();
    Calendar cal = Calendar.getInstance();
    SimpleDateFormat formatter = new SimpleDateFormat("yyyyMMdd");
    Date start_date = new Date();
    Date end_date = new Date();
    Date filename_start_date = new Date();
    Date filename_end_date = new Date();
    String filename_date_string = "";
    String NB_file_name = "";
    String Quote_file_name = "";
	String is_default = "FALSE";
    if (date.equals("")){ //daily
        cal.setTime(today);
		cal.set(Calendar.HOUR_OF_DAY, 23);
        cal.set(Calendar.MINUTE, 59);
        cal.set(Calendar.SECOND, 59);
        cal.set(Calendar.MILLISECOND, 999);
        cal.add(Calendar.DATE, -1);
		end_date = cal.getTime();
		cal.set(Calendar.HOUR_OF_DAY, 0);
        cal.set(Calendar.MINUTE, 0);
        cal.set(Calendar.SECOND,0);
        cal.set(Calendar.MILLISECOND,0);
		cal.add(Calendar.DATE, -58);
        start_date = cal.getTime();

		is_default="TRUE";
    }else{
        cal.setTime(today); // lay khoang date ma daily file co the co data
        cal.add(Calendar.DATE, -1);
		cal.set(Calendar.HOUR_OF_DAY, 23);
        cal.set(Calendar.MINUTE, 59);
        cal.set(Calendar.SECOND, 59);
        cal.set(Calendar.MILLISECOND, 999);
        Date to_date = cal.getTime();
        cal.add(Calendar.DATE, -60);
		cal.set(Calendar.HOUR_OF_DAY, 0);
        cal.set(Calendar.MINUTE, 0);
        cal.set(Calendar.SECOND,0);
        cal.set(Calendar.MILLISECOND,0);
        Date from_date = cal.getTime();
        String[] arrDate = date.split("-"); // lay start, den tu param DATES
        String startDate = arrDate[0];
        String endDate = arrDate[1];
        try {
            start_date = formatter.parse(startDate);
            Date _end_date = formatter.parse(endDate);
			cal.setTime(_end_date);
			cal.set(Calendar.HOUR_OF_DAY, 23);
        	cal.set(Calendar.MINUTE, 59);
        	cal.set(Calendar.SECOND, 59);
        	cal.set(Calendar.MILLISECOND, 999);
			end_date = cal.getTime();
            if(end_date.compareTo(from_date) < 0 || start_date.compareTo(to_date) > 0){ // ngoai khoang ngày
                cal.setTime(start_date);
                cal.add(Calendar.DATE, 61);
                filename_start_date = cal.getTime();

                cal.setTime(end_date);
                cal.add(Calendar.DATE, 61);
                filename_end_date = cal.getTime();

                StringBuilder dates = new StringBuilder();
                Date currentDate = filename_start_date;
                while(currentDate.before(filename_end_date)){
                    String currentDateStr = formatter.format(currentDate);
                    dates.append(currentDateStr);
                    cal.setTime(currentDate);
                    cal.add(Calendar.DATE,1);
                    currentDate = cal.getTime();
                    if(currentDate.before(filename_end_date)) dates.append(";");
                }
                filename_date_string = dates.toString();
            }
            else if (start_date.compareTo(from_date) < 0){ // nua trong nua ngoai
                cal.setTime(start_date);
                cal.add(Calendar.DATE, 61);
                filename_start_date = cal.getTime();

                cal.setTime(today);
                cal.add(Calendar.DATE, -1);
                filename_end_date = cal.getTime();

                StringBuilder dates = new StringBuilder();
                Date currentDate = filename_start_date;
                while(currentDate.before(filename_end_date)){
                    String currentDateStr = formatter.format(currentDate);
                    dates.append(currentDateStr);
                    cal.setTime(currentDate);
                    cal.add(Calendar.DATE,1);
                    currentDate = cal.getTime();
                    if(currentDate.before(filename_end_date)) dates.append(";");
                }
                dates.append("; ");
                filename_date_string = dates.toString();
            }
        } catch (ParseException e) {
            e.printStackTrace();
        }
    }

    String PARAM_INPUT_FILE_NAME = getVariable("PARAM_INPUT_FILE_NAME","r");
    String[] inputFileNameArr = PARAM_INPUT_FILE_NAME.split(";");
    String NB_pattern_file_name = inputFileNameArr[0];
    String Quote_pattern_file_name = inputFileNameArr[1];
    if(filename_date_string.equals("")){
        NB_file_name = NB_pattern_file_name.replace("_yyyyMMdd", "").replace(";", "|");
        Quote_file_name = Quote_pattern_file_name.replace("_yyyyMMdd", "").replace(";", "|");
    }
    else {
        StringBuilder NB_filename = new StringBuilder();
        StringBuilder Quote_filename = new StringBuilder();
        String[] arrDateString = filename_date_string.split(";");
        for(int i = 0; i < arrDateString.length ; i++){
            String NB_tmp = NB_pattern_file_name.replace("yyyyMMdd",arrDateString[i]).replace(";","|");
            String Quote_tmp = Quote_pattern_file_name.replace("yyyyMMdd",arrDateString[i]).replace(";","|");
            if(arrDateString[i].equals(" ")) {
                NB_tmp = NB_tmp.replace("_ ", "");
                Quote_tmp = Quote_tmp.replace("_ ", "");
            }
            NB_filename.append(NB_tmp);
            Quote_filename.append(Quote_tmp);
            if(i < arrDateString.length - 1) {
                NB_filename.append("|");
                Quote_filename.append("|");
            }
        }
        NB_file_name = NB_filename.toString();
        Quote_file_name = Quote_filename.toString();
    }	
// end code
  outputRowData[outputListFilesInfex ] = NB_file_name;
  outputRowData[++outputListFilesInfex ] = Quote_file_name;
  outputRowData[++outputListFilesInfex ] = start_date;
  outputRowData[++outputListFilesInfex ] = end_date;
  outputRowData[++outputListFilesInfex ] = is_default;
  outputRowData[++outputListFilesInfex ] = filename_date_string;
  putRow(data.outputRowMeta, outputRowData);
  return true;
}]]></class_source>
        </definition>
